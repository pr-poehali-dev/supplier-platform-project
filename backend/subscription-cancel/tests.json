    {
      "name": "Cancel subscription - unauthorized",
      "method": "POST",
      "path": "/",
      "body": {
        "subscription_id": "test-id"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "Unauthorized"
      },
      "bodyMatcher": "partial"
    }
  ]
}
